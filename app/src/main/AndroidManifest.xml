<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mmm.comic">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".APP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.mmm.comic.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.mmm.comic.activity.MainActivity" />
        <activity android:name="com.mmm.comic.activity.PayActivity" />
        <activity android:name="com.mmm.comic.activity.KeFuActivity" />
        <activity android:name="com.mmm.comic.activity.WebActivity" />
        <activity android:name="com.mmm.comic.activity.IndexActivity" />

    </application>

</manifest>